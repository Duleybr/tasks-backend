pipeline {
    agent any
    stages{
        stage ('Build Backend'){
            steps{
                sh 'mvn clean package -DskipTests=true'
            }
        }
        stage ('Meio'){
            steps{
                sh 'echo meio'
            }
        }
        stage('Fim'){
            steps{
                sleep (5)
                sh 'echo fim'
            }
        }
    }
}